package boj;

import java.util.Scanner;

public class P10817 {
	private int[] a;
	public int select(int left, int right, int order) {
		if(left==right) return left;
		int pivot= Partition(left, right);
		int k=pivot-left+1;
		if(order==k) {
			return pivot;
		}else if(order<k) return select(left,pivot-1,order);
		else return select(pivot+1,right,order-k);
	}
	P10817(int n1,int n2,int n3){
		this.a[0]=n1;
		this.a[1]=n2;
		this.a[2]=n3;
	}
	private int Partition(int left,int right) {
		int j=left;
		for(int i=left;i<right;i++) {
			if(a[i]<a[right]) {
				swap(a,a[i],a[j++]);
			}
			swap(a,a[j],a[right]);
		}
		return j;
	}
	private void swap(int[] a, int i1, int i2) {
		int tmp = a[i1];
		a[i1]=a[i2];
		a[i2]=tmp;
	}
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		P10817 p = new P10817(sc.nextInt(),sc.nextInt(),sc.nextInt());
		System.out.println(p.select(0, 3, 2));
		
	}

}
