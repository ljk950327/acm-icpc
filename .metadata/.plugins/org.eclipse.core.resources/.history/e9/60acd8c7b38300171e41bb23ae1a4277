package boj;

import java.util.Scanner;

public class P11720 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int sum=0;
		int count=sc.nextInt();
		int num=sc.nextInt();
		for(;count>0;count--) {
			
			sum=num/10^(count-1);
			num-=num/10^count;
		}
		System.out.println(sum);
	}
}
